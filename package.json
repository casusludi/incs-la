{
  "name": "incs-la",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": ""
  },
  "projects": [
    {
      "name": "incs-la",
      "source": "./src",
      "build": "./build",
      "dist": "./dist",
      "bundle": {
        "js": {
          "src": "/index.js",
          "dest": "/bundle.js"
        },
        "css": {
          "src": "/index.scss",
          "dest": "/bundle.css"
        },
        "assets": {
          "src": "/assets",
          "dest": "/assets"
        }
      }
    }
  ],
  "license": "",
  "browser": {},
  "browserify-shim": {},
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "syntax-jsx",
            [
              "transform-react-jsx",
              {
                "pragma": "html"
              }
            ]
          ],
          "ignore": "./node_modules/"
        }
      ]
    ]
  },
  "dependencies": {
    "@cycle/collection": "^0.6.0",
    "@cycle/dom": "^14.1.0",
    "@cycle/http": "^13.3.0",
    "@cycle/isolate": "^1.4.0",
    "@cycle/xstream-run": "^3.1.0",
    "lodash": "^4.17.2",
    "normalize-scss": "^6.0.0",
    "snabbdom-jsx": "^0.3.1",
    "xstream": "^9.0.0"
  },
  "devDependencies": {
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "del": "^2.2.1",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.0.0",
    "gulp-if": "^2.0.1",
    "gulp-jshint": "^2.0.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-template": "^4.0.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "gulp-zip": "^3.2.0",
    "jshint": "^2.9.2",
    "lodash": "^4.13.1",
    "merge-stream": "^1.0.0",
    "minimist": "^1.2.0",
    "path": "^0.12.7",
    "run-sequence": "^1.2.1",
    "snabbdom-jsx": "^0.3.1",
    "stringify": "^5.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
