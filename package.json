{
  "name": "incs-la",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "brunch watch --server -P 1997"
  },
  "projects": [
    {
      "name": "incs-la",
      "source": "./src",
      "build": "./build",
      "dist": "./dist",
      "bundle": {
        "js": {
          "src": "/index.js",
          "dest": "/bundle.js"
        },
        "css": {
          "src": "/index.scss",
          "dest": "/bundle.css"
        },
        "assets": {
          "src": "/assets",
          "dest": "/assets"
        }
      }
    }
  ],
  "license": "",
  "browser": {},
  "browserify-shim": {},
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "syntax-jsx",
            [
              "transform-react-jsx",
              {
                "pragma": "html"
              }
            ]
          ],
          "ignore": "./node_modules/"
        }
      ]
    ]
  },
  "dependencies": {
    "@cycle/collection": "^0.6.0",
    "@cycle/dom": "^17.4.0",
    "@cycle/http": "^13.3.0",
    "@cycle/isolate": "^2.1.0",
    "@cycle/run": "^3.1.0",
    "cycle-onionify": "^3.3.0",
    "cyclic-router": "^4.0.5",
    "lodash": "^4.17.2",
    "normalize-scss": "^6.0.0",
    "snabbdom-jsx": "^0.3.1",
    "switch-path": "^1.2.0",
    "xstream": "^10.8.0"
  },
  "devDependencies": {
    "auto-reload-brunch": "^2.7.1",
    "babel-brunch": "^6.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "brunch": "^2.10.9",
    "clean-css-brunch": "^2.10.0",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-brunch": "^2.0.5",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-nesting": "^4.0.1",
    "postcss-reporter": "^4.0.0",
    "postcss-url": "^7.0.0",
    "stylelint": "^7.11.1",
    "stylelint-config-standard": "^16.0.0",
    "uglify-js-brunch": "^2.10.0"
  }
}
